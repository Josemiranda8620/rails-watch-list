<div class="container">
  <div class="row justify-content-center">
    <div class="col-8 mt-4">
      <h1>New List</h1>
      <%= simple_form_for [@list] do |f| %>
        <%= f.input :name %>
        <%= f.submit 'Create list', class: 'btn btn-danger mt-2' %>
      <% end %>
    </div>
  </div>
</div>
